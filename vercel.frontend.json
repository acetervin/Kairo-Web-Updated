{
  "version": 2,
  "buildCommand": "npm install && npm run build:frontend",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "@vite_api_url",
    "VITE_BACKEND_URL": "@vite_backend_url"
  }
}

